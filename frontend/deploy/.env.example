# Production Environment Variables
# Copy this file to .env and fill in your actual values

# NextAuth authentication secret (required)
# Generate with: openssl rand -base64 32
AUTH_SECRET=your-generated-secret-key-here

# Application URL (required)
# Use https:// for production
NEXTAUTH_URL=https://yourdomain.com

# Domain name for Traefik routing (required)
DOMAIN_NAME=yourdomain.com

# Database path (usually don't change this)
DATABASE_PATH=/app/db/db.production.sqlite3
